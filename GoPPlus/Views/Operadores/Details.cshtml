@model GoPS.Models.Operadores

@{
    ViewBag.Title = "Ver Detalles de Operador";
}

@Html.Hidden("Permiso", "Configuraciones")
<div class="main">
    @{Html.RenderPartial("Breadcrumb", "Operadores > Detalles"); }
    <div class="row">
        <div class="col-lg-12">
            <div>
                <hr />
                <dl class="dl-horizontal">
                    <dt>
                        @Html.DisplayNameFor(model => model.Nombre)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Nombre)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.Numero_Exterior)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Numero_Exterior)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.Numero_Interior)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Numero_Interior)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.Codigo_Postal)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Codigo_Postal)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.Telefono)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Telefono)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.RFC)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.RFC)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.Habilitado)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Habilitado)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.Objetivo_Mensual)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Objetivo_Mensual)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.ID_Afiliado)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Afiliados.Nombre)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.ID_Calle)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Calles.Nombre)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.ID_Turno)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Turnos.Nombre)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.UserID_Operador)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.AspNetUsers.UserName)
                    </dd>

                </dl>
            </div>
            <p class="div_actions">
                @Html.ActionLink("Editar", "Edit", new { id = Model.ID_Operador }, new { @class = "_Edicion" })<span class="_Edicion pipe"> | </span>
                @Html.ActionLink("Regresar", "Index", null, new { @class = "_Visualizacion" })
            </p>
        </div>
    </div>
</div>